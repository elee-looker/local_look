view: huge_strings_dt {
  derived_table: {
    sql: WITH int_table (int_field) AS (
    SELECT 1
    UNION
    SELECT 2
    UNION
    SELECT 3
    UNION
    SELECT 4
    UNION
    SELECT 5
    UNION
    SELECT 6
    UNION
    SELECT 7
    UNION
    SELECT 8
    UNION
    SELECT 9
    UNION
    SELECT 10),
    WITH huge_strings_table (thing) AS (
    SELECT ''
    FROM int_table)
    SELECT * FROM huge_strings_table;;
  }

  dimension: thing {
    type: string
    sql: ${TABLE}.thing ;;
  }
